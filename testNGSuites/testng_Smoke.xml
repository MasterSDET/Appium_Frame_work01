<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

<groups>
    <run>
      <include name="Smoke"/>
    </run>
  </groups>

<listeners>
<listener class-name="org.rahulshettyacademy.TestUtils.Listeners"/>
</listeners>

  <test thread-count="5" name="End to end Framework Test">
    <classes>
      <class name="org.rahulshettyacademy.eCommerce_tc_4_Hybrid"/>     
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Error Validations Test">
    <classes>
      <class name="org.rahulshettyacademy.eCommerce_tc_2"/>     
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="IOS Test">
    <classes>
      <class name="org.rahulshettyacademy.IOSBasics"/>     
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
